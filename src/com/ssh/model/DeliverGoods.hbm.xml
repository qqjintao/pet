<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.DeliverGoods" table="DeliverGoods">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Postal_code" column="Postal_code"/>
		<property name="Store_code" column="Store_code"/>	
		<property name="Price" column="Price"/>
		<property name="Explain" column="Explain"/>
		<property name="Remark" column="Remark"/>
		<property name="Deliver_state" column="Deliver_state"/>
	</class>
</hibernate-mapping>