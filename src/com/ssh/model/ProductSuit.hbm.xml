<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.ProductSuit" table="ProductSuit">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Product_code" column="Product_code"/>
		<property name="Product_name" column="Product_name"/>	
		<property name="Release_time" column="Release_time"/>
		<property name="Market_price" column="Market_price"/>
		<property name="Member_price" column="Member_price"/>
		<property name="Product_tax" column="Product_tax"/>
		<property name="Quantity" column="Quantity"/>
		<property name="Product_weight" column="Product_weight"/>		
		<property name="Product_until" column="Product_until"/>
		<property name="Start_time" column="Start_time"/>
		<property name="End_time" column="End_time"/>
		<property name="Integral" column="Integral"/>
		<property name="Discount_coupon" column="Discount_coupon"/>
	</class>
</hibernate-mapping>