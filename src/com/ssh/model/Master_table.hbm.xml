<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.Master_table" table="Master_table">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Mobile_phone" column="Mobile_phone"/>
		<property name="Head_portrait" column="Head_portrait"/>	
		<property name="Sex" column="Sex"/>
		<property name="Province" column="Province"/>
		<property name="City" column="City"/>
		<property name="Street_address" column="Street_address"/>
		<property name="Profession" column="Profession"/>
		<property name="QQ" column="QQ"/>
		<property name="Personalized_signature" column="Personalized_signature"/>
		<property name="Interest1" column="Interest1"/>
		<property name="Interest2" column="Interest2"/>
		<property name="Interest3" column="Interest3"/>
		<property name="Interest4" column="Interest4"/>
		<property name="Interest5" column="Interest5"/>
	</class>
</hibernate-mapping>