<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.ForumMessages" table="ForumMessages">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="ForumMessages_code" column="ForumMessages_code"/>
		<property name="Mobile_phone" column="Mobile_phone"/>	
		<property name="Forum_code" column="Forum_code"/>
		<property name="ForumMessages_title" column="ForumMessages_title"/>
		<property name="ForumMessages_context" column="ForumMessages_context"/>
		<property name="ForumMessages_time" column="ForumMessages_time"/>
	</class>
</hibernate-mapping>