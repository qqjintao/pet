<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.StoreJurisdiction" table="StoreJurisdiction">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Store_code" column="Store_code"/>
		<property name="Admin_jurisdiction" column="Admin_jurisdiction"/>
		<property name="Jurisdiction_name" column="Jurisdiction_name"/>		
		<property name="Jurisdiction_describe" column="Jurisdiction_describe"/>
	</class>
</hibernate-mapping>