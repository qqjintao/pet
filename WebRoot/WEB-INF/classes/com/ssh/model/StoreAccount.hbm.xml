<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.StoreAccount" table="StoreAccount">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Store_code" column="Store_code"/>
		<property name="Admin_account" column="Admin_account"/>
		<property name="Admin_password" column="Admin_password"/>		
		<property name="Admin_name" column="Admin_name"/>
		<property name="Admin_email" column="Admin_email"/>
		<property name="Admin_phone" column="Admin_phone"/>		
		<property name="Admin_jurisdiction" column="Admin_jurisdiction"/>
		<property name="Registration_state" column="Registration_state"/>
	</class>
</hibernate-mapping>