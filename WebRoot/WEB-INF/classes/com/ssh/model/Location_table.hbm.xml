<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.Location_table" table="Location_table">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Mobile_phone" column="Mobile_phone"/>
		<property name="Name" column="Name"/>	
		<property name="Province" column="Province"/>
		<property name="City" column="City"/>
		<property name="Street" column="Street"/>
		<property name="Phone" column="Phone"/>
		<property name="Fixedline_telephone" column="Fixedline_telephone"/>
		<property name="Zip_code" column="Zip_code"/>
		<property name="Address_code" column="Address_code"/>
	</class>
</hibernate-mapping>