<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.ServiceQQ" table="ServiceQQ">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="QQ_code" column="QQ_code"/>
		<property name="Store_code" column="Store_code"/>
		<property name="QQ_number" column="QQ_number"/>		
		<property name="QQ_state" column="QQ_state"/>
		<property name="Remark" column="Remark"/>
	</class>
</hibernate-mapping>