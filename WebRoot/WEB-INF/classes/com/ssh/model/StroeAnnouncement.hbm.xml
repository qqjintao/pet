<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.StroeAnnouncement" table="StroeAnnouncement">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Announcement_code" column="Announcement_code"/>
		<property name="Store_code" column="Store_code"/>	
		<property name="Announcement_tile" column="Announcement_tile"/>
		<property name="Announcement_content" column="Announcement_content"/>
		<property name="Announcement_time" column="Announcement_time"/>
		<property name="Announcement_state" column="Announcement_state"/>
	</class>
</hibernate-mapping>