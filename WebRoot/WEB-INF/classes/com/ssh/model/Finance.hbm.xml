<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.Finance" table="Finance">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Mobile_phone" column="Mobile_phone"/>
		<property name="Balance" column="Balance"/>	
		<property name="Payment_code" column="Payment_code"/>
		<property name="Integral" column="Integral"/>
		<property name="Discount_coupon" column="Discount_coupon"/>
	</class>
</hibernate-mapping>