<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.Indent" table="Indent">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="Indent_code" column="Indent_code"/>
		<property name="Store_code" column="Store_code"/>	
		<property name="Mobile_phone" column="Mobile_phone"/>
		<property name="Indent_price" column="Indent_price"/>
		<property name="Indent_memberprice" column="Indent_memberprice"/>
		<property name="Indent_time" column="Indent_time"/>
		<property name="Postal_code" column="Postal_code"/>
		<property name="Payment_code" column="Payment_code"/>
		<property name="Location_table_id" column="Location_table_id"/>
		<property name="Integral" column="Integral"/>
		<property name="Indent_state" column="Indent_state"/>
		<property name="Indent_deal1" column="Indent_deal1"/>
		<property name="Indent_time1" column="Indent_time1"/>
		<property name="Indent_deal2" column="Indent_deal2"/>
		<property name="Indent_time2" column="Indent_time2"/>
		<property name="Indent_deal3" column="Indent_deal3"/>
		<property name="Indent_time3" column="Indent_time3"/>
		<property name="Indent_deal4" column="Indent_deal4"/>
		<property name="Indent_time4" column="Indent_time4"/>
		<property name="Evaluate" column="Evaluate"/>
	</class>
</hibernate-mapping>