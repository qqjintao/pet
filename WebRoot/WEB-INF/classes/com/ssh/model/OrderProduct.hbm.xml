<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.model.OrderProduct" table="OrderProduct">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="OrderProduct_code" column="OrderProduct_code"/>
		<property name="Mobile_phone" column="Mobile_phone"/>	
		<property name="ProductSuitId" column="ProductSuitId"/>
		<property name="Store_code" column="Store_code"/>
		<property name="OrderProduct_sum" column="OrderProduct_sum"/>
	</class>
</hibernate-mapping>